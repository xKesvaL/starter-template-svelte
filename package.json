{
  "name": "starter-template-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "vercel deploy --prod",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "homepage": "https://github.com/xKesvaL/starter-template-svelte",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xKesvaL/starter-template-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/xKesvaL/starter-template-svelte/issues"
  },
  "author": "xKesvaL",
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.6",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vercel": "^28.16.1",
    "vite": "^4.0.0"
  },
  "type": "module"
}
